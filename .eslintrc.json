{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "extends": [ "plugin:@wordpress/eslint-plugin/esnext" ],
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "no-unused-vars": 0
    },
    "globals": {
        "$": false
    }
}
